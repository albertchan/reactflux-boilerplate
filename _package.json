{
    "name": "reactflux-boilerplate",
    "version": "0.0.1",
    "description": "Boilerplate for React/Flux app",
    "main": "js/app.js",
    "dependencies": {
        "flux": "^2.0.1",
        "react": "^0.12.2"
    },
    "devDependencies": {
        "browserify": "~8.1.3",
        "envify": "~3.2.0",
        "gulp": "~3.8.11",
        "reactify": "^0.15.2",
        "uglify-js": "~2.4.15",
        "vinyl-source-stream": "latest",
        "watchify": "^2.3.0"
    },
    "scripts": {
        "start": "watchify -o js/bundle.js -v -d .",
        "build": "NODE_ENV=production browserify . | uglifyjs -cm > js/bundle.min.js"
    },
    "browserify": {
        "transform": [
            "reactify",
            "envify"
        ]
    },
    "author": "Albert Chan",
    "license": "Apache 2"
}
